{
  "name": "activityup",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "server.js",
  "engines": {
    "node": "12.12.0"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "clean": "kill -9 $(lsof -t -i:3001)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XtraChase/activityup.git"
  },
  "author": "Chase Richards, Miranda Delong, Cesar Sanchez-Vegas, Jenna Currin, Jeffrey Mann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/XtraChase/activityup/issues"
  },
  "homepage": "https://github.com/XtraChase/activityup#readme",
  "dependencies": {
    "@fullcalendar/core": "^4.3.1",
    "@fullcalendar/daygrid": "^4.3.0",
    "@fullcalendar/interaction": "^4.3.0",
    "@fullcalendar/react": "^4.3.0",
    "@fullcalendar/timegrid": "^4.3.0",
    "@google-cloud/storage": "^4.2.0",
    "@pusher/chatkit-client": "^1.13.4",
    "auth": "0.0.9",
    "aws-amplify-react": "^3.1.3",
    "axios": "^0.19.1",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.4.1",
    "cors": "^2.8.5",
    "css-loader": "^3.4.2",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
    "map": "^1.0.1",
    "mongoose": "^5.8.7",
    "node-sass": "^4.13.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "sass-loader": "^8.0.2",
    "socket.io": "^2.3.0",
    "style-loader": "^1.1.3",
    "underscore.string": "^3.3.5",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "nodemon": "^2.0.2"
  }
}
